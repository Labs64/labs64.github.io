{
    "swaggerVersion": "1.2",
    "apiVersion": "2.0",
    "basePath": "https://netlicensing.labs64.com/core/v2/rest",
    "resourcePath": "/licensee",
    "produces": [
        "application/xml",
        "application/json"
    ],
    "apis": [
        {
            "path": "/licensee/{licenseeNumber}/validate",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Validates active licenses of the licensee",
                    "notes": "",
                    "type": "Licensee",
                    "nickname": "validate",
                    "parameters": [
                        {
                            "name": "licenseeNumber",
                            "description": "licensee number with a maximum length of 1000 characters",
                            "required": true,
                            "type": "string"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 400,
                            "message": "Invalid ID supplied"
                        },
                        {
                            "code": 404,
                            "message": "Licensee not found"
                        }
                    ]
                }
            ]
        }
    ],
    "models": {
        "Licensee": {
            "id": "Licensee",
            "required": [
                "id",
                "name"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64",
                    "description": "Unique identifier for the Pet",
                    "minimum": "0.0",
                    "maximum": "100.0"
                },
                "category": {
                    "$ref": "Category",
                    "description": "Category the pet is in"
                },
                "name": {
                    "type": "string",
                    "description": "Friendly name of the pet"
                },
                "photoUrls": {
                    "type": "array",
                    "description": "Image URLs",
                    "items": {
                        "type": "string"
                    }
                },
                "tags": {
                    "type": "array",
                    "description": "Tags assigned to this pet",
                    "items": {
                        "$ref": "Tag"
                    }
                },
                "status": {
                    "type": "string",
                    "description": "pet status in the store",
                    "enum": [
                        "available",
                        "pending",
                        "sold"
                    ]
                }
            }
        }
    }
}
